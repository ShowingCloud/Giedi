<% if @course_star.errors.blank? %>
var selector = '.course-stars-list';
$.get(location.href, function (html) {
  var doc = $(html).find(selector);
  $(selector).replaceWith(doc);
});
bootbox.hideAll();
<% else %>
var html = "<%= j(render('form', remote: true)) %>";
$('form.new_course_star').replaceWith(html);
<% end %>